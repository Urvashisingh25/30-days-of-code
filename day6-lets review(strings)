# Enter your code here. Read input from STDIN. Print output to STDOUT
def showOutput(word):
    for i in range(0,len(word)):
        if(i%2==0):
            print(word[i],end="")
    print(" ",end="")
    for i in range(1,len(word)):
        if(i%2):
            print(word[i],end="")
    print()
t=int(input())
for i in range(t):
    word=input()
    showOutput(word)
    #print("\n")
